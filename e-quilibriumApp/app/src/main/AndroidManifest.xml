<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.sirdreadlocks.e_quilibrium">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:configChanges="screenSize|orientation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Measures"
            android:screenOrientation="landscape" />
        <activity
            android:name=".PosturalResults"
            android:configChanges="screenSize|orientation"
            android:label="Results" />
        <activity
            android:name=".Settings"
            android:label="Settings" />
        <activity
            android:name=".NewPatient"
            android:label="New Patient" />
        <activity
            android:name=".ListPatients"
            android:label="Patient List"
            android:configChanges="screenSize|orientation" />
        <activity
            android:name=".PatientDetails"
            android:configChanges="screenSize|orientation"
            android:label="@string/title_activity_patient_details"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".FallResults"></activity>
    </application>

</manifest>